{
  "compilerOptions": {
    "target": "ES2020" /* Set the JavaScript language version for emitted JavaScript and */,
    "module": "commonjs" /* Specify what module code is generated. */,
    "esModuleInterop": true /* Emit additional JavaScript to ease support for importing CommonJS */,
    "strict": true /* Enable all strict type-checking options. */,
    "forceConsistentCasingInFileNames": true /* Ensure that casing is consistent in file names. */,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */,
    "noImplicitAny": true /* Raise error on expressions and declarations with an implied 'any' */,
    "noUnusedLocals": true                                 /* Report errors on unused local variables. */,
  }
}
